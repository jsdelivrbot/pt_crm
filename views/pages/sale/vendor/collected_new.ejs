<h3>Add Products Dashboard!</h3>
<div class="well">
  <form action="/sale/collected_new" method="post">
    <div class="row">
      <div class="col-md-6">
        <div class="form-group">
          <label for="col_vendor">Vendor</label>
          <select class="form-control" name="col_vendor" id="col_vendor">
            <option value="">Choose Vendor</option>
            <% for(var ven of vendor){ %>
              <option value="<%= ven.id %>"><%= ven.name %></option>
            <% } %>
          </select>
        </div>
      </div>
      <div class="col-md-6">
        <h6>Products Picked From Vendor</h6>
        <div id="addCollected" class="row"></div>
      </div>
    </div>
    <div class="col-md-12 pull-right">
      <button type="submit" class="btn btn-success">SUBMIT</button>
    </div>
  </form>
  <hr>
  <small>Tick Products To add</small>
  <div id="vendor_table" class="">

  </div>
</div>
<% include ../../../partials/footer %>
