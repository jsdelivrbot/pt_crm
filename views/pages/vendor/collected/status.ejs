<div class="row">
  <div class="col-sm-12">
    <h4>Edit Product IMEI</h4>
  </div>
  <form class="" action="/vendor/collected/status/<%= cprodI.id %>" method="post">
  <div class="col-sm-12">
      <% for( var cprod of cproducts){
          if(cprod.id == cprodI.collected){
             for( var prod of products){
               if(cprod.product == prod.id){
        %>
          <label class="pull-left">Title :: <%= prod.title %></label>
        <% } } } } %>
  </div>
        <div class="col-sm-12">
          <div class="form-group">
            <label for="status">Status</label>
            <select class="status"   name="status">
              <option value="Collected" <% if(cprodI.status=="Collected"){ %> selected <% } %>>Collected</option>
              <option value="Sold" <% if(cprodI.status=="Sold"){ %> selected <% } %>>Sold</option>
              <option value="Returned" <% if(cprodI.status=="Returned"){ %> selected <% } %>>Returned</option>
            </select>
          </div>
        </div>
        <input type="hidden" name="id" value="<%= cprodI.collected %>">
      <div class="clearfix"></div>
      <div class="form-group col-sm-12 padding-15">
        <button type="submit" class="btn btn-success">Submit</button>
      </div>
    </form>
</div>
